{% extends 'GaoAdminBundle::layout.html.twig' %}

{% block datapage %}data-pageId="dispute"{% endblock %}

{% block content_sidebar %}
  {% include "GaoAdminBundle:mod:sidebar.html.twig" with { 'menus': ['dispute-list'] } only %}
{% endblock content_sidebar %}

{% block content_header %}
  Dispute Management Page
  <small></small>
{% endblock content_header %}

{% block content_breadcrumb %}
  {% include "GaoAdminBundle:mod:breadcrumb.html.twig" %}
{% endblock content_breadcrumb %}

{% block contents_main %}
  <div class="row">
    <div class="col-xs-12">
      <div class="box">
        <div class="box-header with-border">
          <h3 class="box-title">Data Table</h3>
        </div>
        <!-- /.box-header -->
        <div class="box-body">
          {% include "GaoAdminBundle:mod:alert.html.twig" with { 'flashbag': app.session.flashbag } %}

          <table id="prg-adminTable" class="table table-bordered table-hover">
            <thead>
              <tr>
                <th>#ID</th>
                <th>Username</th>
                <th>Created</th>
                <th>Status</th>
                <th>Action</th>
              </tr>
            </thead>
            <tbody>
            </tbody>
          </table>
        </div>
        <!-- /.box-body -->
      </div>
      <!-- /.box -->
    </div>
  </div>
{% endblock contents_main %}
