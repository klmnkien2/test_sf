{% extends 'GaoC5Bundle::layout.html.twig' %}

{% block contents_main %}
    <div class="clearfix boxHead">
        <h1 class="title"><i class="glyphicon glyphicon-screenshot"></i> Quản lý tài khoản</h1>
        <div class="desc">Thay đổi mật khẩu...</div>
    </div>

    <div class="clearfix boxEnter">
        <div class="alert alert-info">
            <strong>Info!</strong> Indicates a neutral informative change or action.
        </div>
        <form class="form" role="form">
            <div class="form-group">
                <label for="">Mật khẩu cũ</label>
                <input type="password" class="form-control" id="" placeholder="Mật khẩu cũ">
            </div>
            <div class="form-group">
                <label for="">Mật khẩu mới</label>
                <input type="password" class="form-control" id="" placeholder="Mật khẩu mới">
            </div>
            <div class="form-group">
                <label for="">Nhập lại mật khẩu</label>
                <input type="password" class="form-control" id="" placeholder="Nhập lại mật khẩu">
            </div>
            <div class="btnWrap"><button type="submit" class="btn btn-orange">Submit</button></div>
        </form>
    </div>


{% endblock contents_main %}