{% extends "::base.html.twig" %}
{% block datapage %}data-pageId="no_match"{% endblock %}
{% block meta %}
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  {% block others_meta %}{% endblock others_meta %}
{% endblock meta %}
{% block title %}{% endblock title %}
{% block stylesheets %}
  {% stylesheets filter='cssrewrite'
  output='bundles/gaoc5/css/c5.css'

  'bundles/gaoc5/css/bootstrap.min.css'
  'bundles/gaoc5/css/theme.css'
  'bundles/gaoc5/css/layout.css'
  'bundles/gaoc5/css/style.css'
  'bundles/gaoc5/plugin/sweetalert/sweetalert.css'%}
  <link rel="stylesheet" type="text/css" href="{{ asset_url }}"/>
  {% endstylesheets %}
{% endblock stylesheets %}
{% block noscript %}
  <p>This site using javascript, please enable it.</p>
{% endblock noscript %}
{% block header %}
  <div id="UIhead" class="navbar-fixed-top">
    <div class="primary">
      <div class="container">
        <div class="row">
          <div class="col-xs-12">
            <div class="navbar-header">
              <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse" title="Toggle Menu">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
              </button>
              <a class="navbar-brand" href="{{ path('gao_c5_about') }}" title="C5">C5</a>
            </div>

            <div class="btnList pull-right">
                {% if not is_granted('ROLE_USER') %}
				  <a class="btn btn-default" href="{{ path('login') }}" title="Login">Đăng nhập</a>
				{% else %}
				<a class="btn btn-orange" href="{{ path('gao_c5_homepage') }}" title="Login">Giao dịch</a>
				{% endif %}
			</div>

          </div>
        </div>
      </div>
    </div>

    <div class="navMain">
      <div class="container">
        <div class="row">
          <div class="col-xs-12">
            <div class="navbar-collapse collapse">
            </div>
          </div>
        </div>
      </div>
    </div>
  </div><!-- //#UIhead -->
{% endblock header %}

{% block contents %}
  <div id="UIcont" class="UIcont">
      {% block contents_main %}
      {% endblock contents_main %}
  </div>

{% endblock contents %}

{% block footer %}
  <div id="UIfoot">
    <div class="copyright">
      <div class="container">
        <div>Copyright &copy; 2016 <a class="brand" href="index.html">C5</a>. All rights reserved.</div>
      </div>
    </div>
  </div><!-- //#UIfoot -->
{% endblock footer %}

{% block javascripts %}

  {% javascripts
  output='bundles/gaoc5/js/c5.js'

  '@GaoC5Bundle/Resources/public/js/library/jquery/jquery-1.11.0.min.js'
  '@GaoC5Bundle/Resources/public/js/library/bootstrap/bootstrap.min.js'

  '@GaoC5Bundle/Resources/public/plugin/jQueryFileUpload/js/vendor/jquery.ui.widget.js'
  '@GaoC5Bundle/Resources/public/plugin/jQueryFileUpload/js/jquery.iframe-transport.js'
  '@GaoC5Bundle/Resources/public/plugin/jQueryFileUpload/js/jquery.fileupload.js'
  '@GaoC5Bundle/Resources/public/plugin/jQueryLoadingIndicator/jquery.loading-indicator.min.js'
  '@GaoC5Bundle/Resources/public/plugin/sweetalert/sweetalert.min.js'

  '@GaoC5Bundle/Resources/public/js/library/joo/joo.js'
  '@GaoC5Bundle/Resources/public/js/library/routing/routing.js'

  '@GaoC5Bundle/Resources/public/js/app/no_match/mapping.js'
  '@GaoC5Bundle/Resources/public/js/app/common/widget/GlobalSetup.js'
  '@GaoC5Bundle/Resources/public/js/app/common/mapping.js'
  '@GaoC5Bundle/Resources/public/js/app/default/widget/Pd.js'
  '@GaoC5Bundle/Resources/public/js/app/default/mapping_pd.js'  
  '@GaoC5Bundle/Resources/public/js/app/default/widget/Gd.js'
  '@GaoC5Bundle/Resources/public/js/app/default/mapping_gd.js'
  '@GaoC5Bundle/Resources/public/js/app/default/widget/History.js'
  '@GaoC5Bundle/Resources/public/js/app/default/mapping_history.js'
  '@GaoC5Bundle/Resources/public/js/app/default/widget/UploadImage.js'
  '@GaoC5Bundle/Resources/public/js/app/default/mapping_dispute.js'

  '@GaoC5Bundle/Resources/public/js/app/routing.js'%}
  <script type="text/javascript" src="{{ asset_url }}"></script>

  {% endjavascripts %}
    <script>
      //Some global things
    </script>
{% endblock javascripts %}

